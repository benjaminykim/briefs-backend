const { Sequelize, DataTypes } = require('sequelize');

module.exports = (sequelize) => {
	const Stub = sequelize.define('stub', {
		url: {
			type: DataTypes.STRING,
			unique: true,
			allowNull: false,
		},
		hit: {
			type: DataTypes.INTEGER,
			defaultValue: 0,
		},
	},
	{
		freezeTableName: true
	});
	return Stub;
};
